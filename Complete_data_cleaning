   "source": [
    "# abcabc >> abcabcabcabc 要第一个abc, 从index1开始找\n",
    "def deduplication_better(x):\n",
    "    trim = x.strip() # 去掉左右的空白字符\n",
    "    index = (trim+trim).find(trim,1) # 从第一位开始找trim\n",
    "    if index != -1: \n",
    "        return trim[:index]\n",
    "    else:\n",
    "        return 'unknown'"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [],
   "source": [
    "df_admin['location'] = df_admin['location'].apply(deduplication_better)"
   ]
